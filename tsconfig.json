{
  "include": [
    "assets/js/vue/**/*.*"
  ],
  "compilerOptions": {
    "outDir": "./built/",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "module": "es2015",
    "moduleResolution": "node",
    "lib": ["es6", "dom", "es2017.object", "es2016.array.include"],
    "target": "es5"
  }
}
